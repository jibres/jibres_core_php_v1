<?xml version="1.0" encoding="utf-8" standalone="no"?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <update>
      <domain:update xmlns:domain="http://epp.nic.ir/ns/domain-1.0">
        <domain:name>JIBRES-SAMPLE-DOMAIN.IR</domain:name>
        <domain:chg>
          <domain:contact type="holder">JIBRES-NIC-ACCOUNT</domain:contact>
          <domain:contact type="admin">JIBRES-NIC-ACCOUNT</domain:contact>
          <domain:contact type="tech">JIBRES-NIC-ACCOUNT</domain:contact>
          <domain:contact type="bill">JIBRES-NIC-ACCOUNT</domain:contact>
        </domain:chg>
        <domain:authInfo>
          <domain:pw>JIBRES-TOKEN</domain:pw>
        </domain:authInfo>
      </domain:update>
    </update>
    <extension>
      <pin pinCode="JIBRES-PIN" />
    </extension>
    <clTRID>JIBRES-TRACKING-NUMBER</clTRID>
  </command>
</epp>
