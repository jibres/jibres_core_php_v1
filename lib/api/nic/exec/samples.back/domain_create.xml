<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
 <command>
  <create>
   <domain:create xmlns:domain="http://epp.nic.ir/ns/domain-1.0">
    <domain:name>JIBRES-SAMPLE-DOMAIN.IR</domain:name>
    <domain:period unit="m">60</domain:period>
    <domain:ns>
     <domain:hostAttr>
      <domain:hostName>ns1.JIBRES-SAMPLE-DOMAIN.IR</domain:hostName>
